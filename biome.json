{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": ["dist", ".vscode", "tsconfig.json", "tsconfig.app.json", "tsconfig.node.json", "tsconfig.json"]
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab",
		"lineWidth": 120
	},
	"linter": {
		"enabled": false
	},
	"json": {
		"parser": {
			"allowComments": true
		}
	}
}
